{% if settings.enable_twl_aw %}
<main class="{{page.title | handle }} merged-page-template">  
  {% if settings.enable_feel_banner_img_complex_k_aw %}  
  <section id="page-header">
    <div class="header-background compexpagebn desktop-stance">
        <a href="{{ settings.stance_feel_link_complex_k_aw }}" class="changable-image">
        <img src="{{ settings.feel_banner_img_complex_k_aw | img_url: 'master' }}" >
        <div class="page-width">
          <div class="grid">
            <div class="grid__item">
              {%if settings.enable_kkpgtit_bandhead_aw %}
              <h1>{{ page.title }}</h1>
              {% endif %}
              {% if settings.kingkongpg_customhead_text_aw %}
              <h1>{{ settings.kingkongpg_customhead_text_aw }}</h1>
              {% endif %}
            </div>
          </div>
        </div>
       </a>
    </div>
    <div class="header-background mobile-stance">
        <a href="{{ settings.stance_feel_link_complex_k_aw }}" class="changable-image">
        <img src="{{ settings.feel_banner_img_complex_k_mobile_aw | img_url: 'master' }}" />
        <div class="page-width">
          <div class="grid">
            <div class="grid__item">
              {%if settings.enable_kkpgtit_bandhead_aw %}
              <h1>{{ page.title }}</h1>
              {% endif %}
              {%if settings.kingkongpg_customhead_text_aw %}
              <h1>{{ settings.kingkongpg_customhead_text_aw }}</h1>
              {% endif %}
            </div>
          </div>
        </div>
       </a>
    </div>
  </section>
  {% endif %}  
  {%if  settings.enable_shop_top_compex_k_aw %}
  <div class="section page">
    <div class="page-width">
      <div class="content">
        <div class="text-center new-division"> 
          <h1 class="">{{ settings.top_compex_k_aw }}</h1>
        </div>
        <div class="stance-view-all-button top-compex text-center">    
          <a href="{{ settings.top_compex_link11_k_aw }}">{{ settings.stance_view_all_buy_complex11_k_aw }}</a>
        </div>
      </div>
    </div>
  </div>
  {% endif %}
  {% if settings.king_kong_countdown_aw %}
  {% include 'countdown' %} 
  {% endif %}
  
    {% if settings.enable_slider_1 %}
       <div class="11 a-set-first-collection">
        {% assign actual_product = product %}
        {% assign vendor = product.vendor %}
        {% assign vendor_handle = vendor | handleize %}
        {% assign handle = product.handle %}
        {% assign brand_products = "" %}
        {% for product in collections[settings.twl_aw_coll].products %}
        {% capture temp %}
        {{product.handle}}
        {% if forloop.last == false %},{% endif%}
        {% endcapture %}
        {% assign brand_products = brand_products | append: temp %}
        {% endfor %}
        {% assign product_handles = brand_products | split: ","%}
        {% if product_handles.size > 0 %}
        <section id="by-brand">
          <div class="page-width">
            <div class="grid">
              <div class="grid__item">
                <div class="division-wrap">
                  <h1 class="division-title">
                    {{ settings.aw_coll_heading }}
                  </h1>
                </div>
              </div>
            </div>
            <div class="">
              <div class="swiper-container slider-carousel-wrap">
                <div class="swiper-wrapper collection-items-holder hot-deals auto-load-col center_reviews">
                  {% assign counter = 0  %}
                  {% assign max_products = 8 | times: 1 %}
                  {% for handle in product_handles %}
                  {% assign counter = counter | plus: 1  %}
                  {% if counter <= max_products %}
                  {% assign product_handle = handle | handleize %}
                  {% assign product = all_products[product_handle] %}
                  <div class="swiper-slide  extra-small--one-whole small--one-half medium-up--one-quarter">
                    {% include 'a-product-card-grid-2' %}
                  </div>
                  {% endif %}
                  {% endfor %}
                </div>
                <!-- Add Arrows -->
                <div class="swiper-button-next"><i class="fa fa-angle-right"></i></div>
                <div class="swiper-button-prev"><i class="fa fa-angle-left"></i></div>
                <div class="stance-view-all-button">
                  <a href="{{ settings.top_collink }}">{{ settings.coll_button_text }}</a>
                </div>
              </div>
            </div>
          </div>
        </section>
        {% endif %}
      {% assign product  = actual_product %}
    </div>
   {% endif %}
  
    {% if settings.enable_slider_2 %}
    <div class="a-set-second-collection">
      {% assign actual_product = product %}
      {% assign vendor = product.vendor %}
      {% assign vendor_handle = vendor | handleize %}
      {% assign handle = product.handle %}
      {% assign brand_products = "" %}
      {% for product in collections[settings.twl_aw_coll2].products %}
      {% capture temp %}
      {{product.handle}}
      {% if forloop.last == false %},{% endif%}
      {% endcapture %}
      {% assign brand_products = brand_products | append: temp %}
      {% endfor %}
      {% assign product_handles = brand_products | split: ","%}
      {% if product_handles.size > 0 %}
      <section id="by-brand">
        <div class="page-width">
          <div class="grid">
            <div class="grid__item">
              <div class="division-wrap">
                <h1 class="division-title">
                  {{ settings.aw_coll_heading2}}
                </h1>
              </div>
            </div>
          </div>
          <div class="">
            <div class="swiper-container slider-carousel-wrap">
              <div class="swiper-wrapper collection-items-holder hot-deals auto-load-col center_reviews">
                {% assign counter = 0  %}
                {% assign max_products = 5 | times: 1 %}
                {% for handle in product_handles %}
                {% assign counter = counter | plus: 1  %}
                {% if counter <= max_products %}
                {% assign product_handle = handle | handleize %}
                {% assign product = all_products[product_handle] %}
                <div class="swiper-slide extra-small--one-whole small--one-half medium-up--one-quarter">
                  {% include 'a-product-card-grid-2' %}
                </div>
                {% endif %}
                {% endfor %}
              </div>
              <!-- Add Arrows -->
              <div class="swiper-button-next"><i class="fa fa-angle-right"></i></div>
              <div class="swiper-button-prev"><i class="fa fa-angle-left"></i></div>
              <div class="stance-view-all-button">
                <a href="{{ settings.top_collink2 }}">{{ settings.coll_button_text2 }}</a>
              </div>
            </div>
          </div>
        </div>
      </section>
      {% endif %}
      {% assign product  = actual_product %}
    </div>
    {% endif %}
  
  
